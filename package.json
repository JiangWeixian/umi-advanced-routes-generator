{
  "name": "umi-plugin-pro-routes",
  "version": "1.0.0",
  "description": "modal page gallery plugin for umi",
  "authors": {
    "name": "jiangweixian"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/JiangWeixian/umi-advanced-routes-generator"
  },
  "peerDependencies": {
    "umi": "2.9.6",
    "react": "16.9.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "umi-tools build",
    "dev": "umi dev --prefix example",
    "prepublishOnly": "npm run build && np --no-cleanup --yolo --no-publish --any-branch",
    "update": "npm update && npm update --save-dev",
    "prettier": "prettier --config .prettierrc --write '**/*'"
  },
  "devDependencies": {
    "@types/mustache": "0.8.32",
    "husky": "3.0.4",
    "np": "3.0.4",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.5.3",
    "umi-build-dev": "^1.11.3",
    "umi-tools": "*",
    "umi-types": "^0.3.17"
  },
  "files": [
    "lib",
    "src",
    "template"
  ],
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "prettier --config .prettierrc --loglevel warn --write '**/*'"
    }
  },
  "dependencies": {
    "mustache": "3.0.1",
    "path-is-root": "0.1.0",
    "umi-utils": "1.4.1"
  }
}
